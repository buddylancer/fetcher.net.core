<?xml version="1.0" encoding="utf-8"?>
<rewrite>
    <rules>
        <rule name="image">
            <match url="^images/$" ignoreCase="false"/>
            <action type="Rewrite" url="page404.html"/>
        </rule>
        <rule name="rss">
            <match url="^rss[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="rss"/>
        </rule>
        <rule name="rss/?.xml">
            <match url="^rss/([A-Za-z0-9\.]*).xml*$" ignoreCase="false"/>
            <action type="Rewrite" url="rss?filter={R:1}" appendQueryString="false"/>
        </rule>
        <rule name="rss/filter/?">
            <match url="^rss/filter/([A-Za-z0-9\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="rss?filter={R:1}" appendQueryString="false"/>
        </rule>
        <rule name="rss/source/?">
            <match url="^rss/source/([A-Za-z0-9\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="rss?source={R:1}" appendQueryString="false"/>
        </rule>
        <rule name="rss/source/?/filter/?">
            <match url="^rss/source/([A-Za-z0-9\.]*)/filter/([A-Za-z0-9\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="rss?source={R:1}&amp;filter={R:2}" appendQueryString="false"/>
        </rule>
        <rule name="rss/filtes/?/source/?">
            <match url="^rss/filter/([A-Za-z0-9\.]*)/source/([A-Za-z0-9\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="rss?filter={R:1}&amp;source={R:2}" appendQueryString="false"/>
        </rule>
        <rule name="items/source/?/filter/?/list/?">
            <match url="^items/source/([A-Za-z0-9\.]*)/filter/([A-Za-z0-9\.]*)/list/([0-9]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=items&amp;source={R:1}&amp;filter={R:2}&amp;list={R:3}" appendQueryString="false"/>
        </rule>
        <rule name="items/filter/?/source/?/list/?">
            <match url="^items/filter/([A-Za-z0-9\.]*)/source/([A-Za-z0-9\.]*)/list/([0-9]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=items&amp;filter={R:1}&amp;source={R:2}&amp;list={R:3}" appendQueryString="false"/>
        </rule>
        <rule name="items/source/?/filter/?">
            <match url="^items/source/([A-Za-z0-9\.]*)/filter/([A-Za-z0-9\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=items&amp;source={R:1}&amp;filter={R:2}" appendQueryString="false"/>
        </rule>
        <rule name="items/filtes/?/source/?">
            <match url="^items/filter/([A-Za-z0-9\.]*)/source/([A-Za-z0-9\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=items&amp;filter={R:1}&amp;source={R:2}" appendQueryString="false"/>
        </rule>
        <rule name="items/source/?/list/?">
            <match url="^items/source/([A-Za-z0-9\.]*)/list/([0-9]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=items&amp;source={R:1}&amp;list={R:2}" appendQueryString="false"/>
        </rule>
        <rule name="items/source/?">
            <match url="^items/source/([A-Za-z0-9\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=items&amp;source={R:1}" appendQueryString="false"/>
        </rule>
        <rule name="items/filter/?/list/?">
            <match url="^items/filter/([A-Za-z0-9\.]*)/list/([0-9]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=items&amp;filter={R:1}&amp;list={R:2}" appendQueryString="false"/>
        </rule>
        <rule name="items/filter/?">
            <match url="^items/filter/([A-Za-z0-9\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=items&amp;filter={R:1}" appendQueryString="false"/>
        </rule>
        <rule name="items/list/?">
            <match url="^items/list/([0-9]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=items&amp;list={R:1}" appendQueryString="false"/>
        </rule>
        <rule name="items">
            <match url="^items[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=items" appendQueryString="false"/>
        </rule>
        <rule name="sources">
            <match url="^sources[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=sources" appendQueryString="false"/>
        </rule>
        <rule name="item/?/?">
            <match url="^item/([0-9]*)/([A-Za-z0-9\-\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=view_item&amp;id={R:1}&amp;title={R:2}" appendQueryString="false"/>
        </rule>
        <rule name="item/?">
            <match url="^item/([0-9]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="index?p=view_item&amp;id={R:1}" appendQueryString="false"/>
        </rule>
        <rule name="redirect/item/?">
            <match url="^redirect/item/([0-9]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="action?p=do_redirect_item&amp;id={R:1}" appendQueryString="false"/>
        </rule>
        <rule name="redirect/item/?/?">
            <match url="^redirect/item/([0-9]*)/([A-Za-z0-9\-\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="action?p=do_redirect_item&amp;id={R:1}&amp;title={R:2}" appendQueryString="false"/>
        </rule>
        <rule name="redirect/source/?">
            <match url="^redirect/source/([A-Za-z0-9\.]*)[/]*$" ignoreCase="false"/>
            <action type="Rewrite" url="action?p=do_redirect_source&amp;source={R:1}" appendQueryString="false"/>
        </rule>
    </rules>
</rewrite>